{
  "presets": ["react", "es2015"],
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-rest-spread",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "lib": "./src/lib",
          "styles": "./src/styles"
        }
      }
    ]
  ]
}
